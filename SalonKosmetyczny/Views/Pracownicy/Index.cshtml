@model IEnumerable<SalonKosmetyczny.Models.DbModels.Pracownik>

@{
    ViewBag.Title = "Pracownicy";
}




<h2>Pracownicy</h2>

<p>
    @Html.ActionLink("Dodaj pracownika", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ImiePracownika)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NazwiskoPracownika)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ImiePracownika)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NazwiskoPracownika)
            </td>
            <td>
                @Html.ActionLink("Edytuj", "Edit", new { id = item.PracownikId }) |
                @Html.ActionLink("Szczegóły", "Details", new { id = item.PracownikId }) |
                @Html.ActionLink("Usuń", "Delete", new { id = item.PracownikId })
            </td>
        </tr>
    }

</table>
